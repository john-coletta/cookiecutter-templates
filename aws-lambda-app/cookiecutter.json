{
    "project_name": "my-lambda-app",
    "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '-').replace('_', '-') }}",
    "description": "AWS Lambda application for processing data",
    "author_name": "Your Name",
    "author_email": "your.email@example.com",
    "aws_region": "us-east-1",
    "python_version": ["3.11", "3.10", "3.9"],

    "main_handler": "email_processor",
    "main_service": "email_service",
    "main_model": "email_model",
    "integration_test_name": "email_flow",

    "use_api_gateway": "y",
    "use_s3_trigger": "y",
    "use_eventbridge": "y",
    "use_dynamodb": "n",
    "use_rds": "n",

    "infrastructure_tool": ["sam", "cloudformation", "terraform"],
    "include_terraform": "n",
    "include_docker": "y",
    "include_github_actions": "y",

    "log_level": ["INFO", "DEBUG", "WARNING", "ERROR"],
    "memory_size": "256",
    "timeout": "30",

    "environment_stages": ["dev", "staging", "prod"]
}